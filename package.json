{
  "name": "@apexdevlabs/contentstack",
  "version": "1.0.0",
  "description": "Contentstack utilities: schema validation with Zod and entry to Markdown conversion",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./schema": {
      "import": {
        "types": "./dist/schema/index.d.ts",
        "default": "./dist/schema/index.js"
      },
      "require": {
        "types": "./dist/schema/index.d.cts",
        "default": "./dist/schema/index.cjs"
      }
    },
    "./markdown": {
      "import": {
        "types": "./dist/markdown/index.d.ts",
        "default": "./dist/markdown/index.js"
      },
      "require": {
        "types": "./dist/markdown/index.d.cts",
        "default": "./dist/markdown/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "publint": "publint",
    "size": "npm run build && du -sh dist",
    "prepublishOnly": "npm run build && npm run publint"
  },
  "keywords": [
    "contentstack",
    "zod",
    "schema",
    "validation",
    "typescript",
    "content-type",
    "headless-cms",
    "cms",
    "markdown",
    "entry"
  ],
  "author": "ApexDevLabs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apexdevlabs/contentstack.git"
  },
  "bugs": {
    "url": "https://github.com/apexdevlabs/contentstack/issues"
  },
  "homepage": "https://github.com/apexdevlabs/contentstack#readme",
  "peerDependencies": {
    "zod": ">=3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "publint": "^0.3.16",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4",
    "zod": "^4.3.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
